{
	"manifest_version":2,
	"name":"InvenAdClick",
	"description":"인벤 광클을 쉽고 빠르게!",
	"version":"1.0.0",
	"permissions":["storage","webRequest","webRequestBlocking","http://*/*"],
	"icons":
	{
		"16":"icon.png"
	},
	"browser_action":
	{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"background":
	{
		"scripts":["background.js"]
	},
	"content_scripts":
	[{
		"matches":["http://*.inven.co.kr/*"],
		"js":["content.js"]
	},
	{
		"matches":["http://adn.inven.co.kr/*"],
		"all_frames":true,
		"js":["iframe.js"]
	}],
	"web_accessible_resources":["adClickAll.js","domain.js"]
}